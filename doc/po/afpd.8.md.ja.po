#: manpages/man8/afpd.8.md:103
#, no-wrap
msgid "*extmap.conf*\n"
msgstr ""

#. type: Plain text
#: manpages/man8/afpd.8.md:105
#, no-wrap
msgid "> file name extension mapping\n"
msgstr "> ファイル名拡張子マッピング\n"

#. type: Plain text
#: manpages/man8/afpd.8.md:107
#, no-wrap
msgid "*message.pid*\n"
msgstr ""

#. type: Plain text
#: manpages/man8/afpd.8.md:109
#, no-wrap
