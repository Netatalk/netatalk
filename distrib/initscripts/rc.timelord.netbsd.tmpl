#!/bin/sh
#
# PROVIDE: timelord
# REQUIRE: atalkd
#
# AppleTalk time server daemon. (netatalk :NETATALK_VERSION:)
# Make sure not to start atalkd in the background:
# its data structures must have time to stablize before running the
# other processes.
#

. /etc/rc.subr

name="timelord"
rcvar=$name
command=":SBINDIR:/timelord"

load_rc_config $name
run_rc_command "$1"

